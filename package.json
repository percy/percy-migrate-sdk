{
  "name": "@percy/migrate",
  "description": "Easily upgrade and migrate your Percy SDK to the latest version",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "percy-migrate": "bin/run.cjs"
  },
  "files": [
    "bin",
    "dist",
    "transforms"
  ],
  "engines": {
    "node": ">=14"
  },
  "@percy/cli": {
    "commands": [
      "./dist/index.js"
    ]
  },
  "scripts": {
    "build": "babel --root-mode upward src --copy-files --out-dir dist",
    "clean": "rm -rf packages/**/{dist,.nyc_output,coverage,oclif.manifest.json}",
    "lint": "eslint --ignore-path .gitignore .",
    "readme": "./scripts/readme",
    "test": "yarn test:env jasmine --config=./test/jasmine.json",
    "test:env": "cross-env NODE_ENV=test NODE_OPTIONS='--loader=./test/loader.js' DUMP_FAILED_TEST_LOGS=1",
    "test:coverage": "nyc yarn test"
  },
  "publishConfig": {
    "access": "public"
  },
  "mocha": {
    "require": [
      "@babel/register",
      "./test/helpers/setup-codeshift.js"
    ],
    "timeout": 10000,
    "recursive": true
  },
  "dependencies": {
    "@percy/cli-command": "^1.1.0",
    "@percy/config": "^1.1.0",
    "@percy/logger": "^1.1.0",
    "cross-spawn": "^7.0.3",
    "inquirer": "^8.0.0",
    "inquirer-glob-prompt": "^0.1.0",
    "semver": "^7.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/eslint-parser": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/register": "^7.11.5",
    "babel-plugin-istanbul": "^6.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "expect": "^27.0.2",
    "jasmine": "^4.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "memfs": "^3.4.3",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0"
  },
  "volta": {
    "node": "14.19.3"
  }
}
